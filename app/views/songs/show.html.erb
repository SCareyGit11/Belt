<h3>Others who added <%=@song.artist%> - <%=@song.title%></h3>

<table>
<thead>
	<th>Name</th>
	<th>Times Added</th>
</thead>
<tbody>
	<% if @song.users %>
	<% @song.users.each do |user| %>
	<tr>
		<td> <a href="/users/<%=user.id%>"> <%=user.first_name%> <%=user.last_name%> </a> </td>
		<td> <%=@song.adds.where(user_id:user.id).count%> </td>
	</tr>
	<% end %>
	<% end %>
</tbody>
</table>

<a href="/songs/index">Dashboard</a>

